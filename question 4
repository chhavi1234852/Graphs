#include <iostream>
using namespace std;
int adj[100][100];
int dist[100];
int visited[100];
int main() {
    int V, E;
    cin >> V >> E;
for (int i = 0; i < V; i++)
        for (int j = 0; j < V; j++)
            adj[i][j] = 0;
    for (int i = 0; i < E; i++) {
        int u, v, w;
        cin >> u >> v >> w;
        adj[u][v] = w;
        adj[v][u] = w;
    }
int start;
    cin >> start;
 for (int i = 0; i < V; i++) {
        dist[i] = 999999;
        visited[i] = 0;
    }
 dist[start] = 0;
 for (int i = 0; i < V; i++) {
        int u = -1, mind = 999999;
for (int j = 0; j < V; j++)
            if (!visited[j] && dist[j] < mind) {
                mind = dist[j];
                u = j;
            }
visited[u] = 1;
 for (int v = 0; v < V; v++) {
            if (adj[u][v] != 0 && dist[u] + adj[u][v] < dist[v]) {
                dist[v] = dist[u] + adj[u][v];
            }
        }
    }
 for (int i = 0; i < V; i++)
        cout << dist[i] << " ";
return 0;
}
